{
  "name": "openapi3-to-mongose-typescript",
  "version": "0.1.1",
  "description": "Generate Mongoose Schema from OpenAPI 3 Specification yaml files (docs in https://swagger.io/docs/specification/about/)",
  "main": "./lib/src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc && chmod +x ./lib/src/index.js",
    "up": "rm -rf ./lib/* && tsc && chmod +x ./lib/src/index.js && npm publish",
    "run": "tsc && node ./lib/src/index.js"
  },
  "bin": {
    "openapi3-to-mongose-typescript": "./lib/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/fran.fig/openapi3-to-mongose-typescript.git"
  },
  "keywords": [
    "mongoose",
    "openapi3",
    "swagger",
    "typescript",
    "database",
    "mongo",
    "mongodb",
    "yaml"
  ],
  "dependencies": {
    "@types/mongoose": "^5.5.3",
    "args": "^5.0.1",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.4",
    "js-yaml": "^3.13.1",
    "moment": "^2.24.0",
    "mongoose": "^5.5.11",
    "mongoose-paginate": "^5.0.3",
    "path": "^0.12.7",
    "yaml": "^1.6.0"
  },
  "devDependencies": {
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "recast": "^0.18.1",
    "shelljs": "^0.8.3",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "typescript": "^3.4.5",
    "typescript-parser": "^2.6.1"
  },
  "author": "Francisco Figueiredo",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/fran.fig/openapi3-to-mongose-typescript/issues"
  },
  "homepage": "https://gitlab.com/fran.fig/openapi3-to-mongose-typescript#readme"
}
